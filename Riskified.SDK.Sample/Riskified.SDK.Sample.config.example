<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1" />
  </startup>
  <applicationSettings>
    <add key="NotificationsWebhookUrl" value="[http/https]://[your url here]:[your port here]/[your webhook path]/"/>
    <add key="MerchantDomain" value="[your shop domain as registered to riskified]"/>
    <add key="MerchantAuthenticationToken" value="[Your authentication token string]"/>
    <add key="RiskifiedOrderWebhookUrl" value="[Riskified url for sending orders to]"/>
    <add key="RiskifiedMerchantNotificationsWebhookRegistrationUrl" value="[Riskified url for sending webhook-registration for notifications to]"/>
  </applicationSettings>
</configuration>